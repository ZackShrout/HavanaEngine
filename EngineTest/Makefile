# #############################################

GENERATED :=
OBJECTS :=

GENERATED += $(OBJDIR)/Lights.o
GENERATED += $(OBJDIR)/Main.o
GENERATED += $(OBJDIR)/RenderItem.o
GENERATED += $(OBJDIR)/Scripts.o
GENERATED += $(OBJDIR)/ShaderCompilation.o
GENERATED += $(OBJDIR)/TestRenderer.o
GENERATED += $(OBJDIR)/TestRendererLinux.o
GENERATED += $(OBJDIR)/TestRendererWin32.o
