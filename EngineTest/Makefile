OBJECTS += $(OBJDIR)/Main.o
OBJECTS += $(OBJDIR)/RenderItem.o
OBJECTS += $(OBJDIR)/Scripts.o
OBJECTS += $(OBJDIR)/ShaderCompilation.o
OBJECTS += $(OBJDIR)/TestRenderer.o
OBJECTS += $(OBJDIR)/TestRendererLinux.o
